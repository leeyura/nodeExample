doctype html
html
	head
		meta(charset='utf-8')
	body
		h1 
			a(href='/main') File Insert
		ul
			each title in titles
				li
					a(href='/title/'+title)= title
		artcle
		form(action='/main' method='post')
			p
				input(type='text' name='title' placeholder='title')
			p
				textarea(name='content')
			p
				input(type='submit')
